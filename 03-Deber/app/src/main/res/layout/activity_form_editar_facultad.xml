<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".formEditarFacultad">
    <EditText
        android:id="@+id/tv_nombreMod"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="96dp"
        android:ems="10"
        android:hint="Nombre"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/tv_versionMod"
        app:layout_constraintEnd_toEndOf="@+id/tv_versionMod"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tv_versionMod" />

    <EditText
        android:id="@+id/tv_versionMod"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="80dp"
        android:ems="10"
        android:hint="Fecha Fundacion"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/tv_almacenamientoMod"
        app:layout_constraintEnd_toEndOf="@+id/tv_almacenamientoMod"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tv_almacenamientoMod" />

    <EditText
        android:id="@+id/tv_almacenamientoMod"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="44dp"
        android:ems="10"
        android:hint="Numero Estudiantes"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/btn_editar_facultad"
        app:layout_constraintEnd_toEndOf="@+id/btn_editar_facultad"
        app:layout_constraintStart_toStartOf="@+id/btn_editar_facultad" />

    <Button
        android:id="@+id/btn_editar_facultad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Editar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.773" />
</androidx.constraintlayout.widget.ConstraintLayout>